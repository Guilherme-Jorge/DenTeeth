<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/EmergenciaFragment">

    <fragment
        android:id="@+id/EmergenciaFragment"
        android:name="br.edu.puccampinas.denteeth.emergencia.EmergenciaFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_emergencia">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="br.edu.puccampinas.denteeth.emergencia.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/EmergenciaFragment" />
    </fragment>
</navigation>